<application>
  <component name="CodeInsightSettings">
    <option name="USE_INSTANCEOF_ON_EQUALS_PARAMETER" value="true" />
  </component>
  <component name="CodeVisionSettings">
    <option name="codeVisionGroupToPosition">
      <map>
        <entry key="inheritors" value="Default" />
        <entry key="problems" value="Default" />
        <entry key="references" value="Default" />
        <entry key="vcs.code.vision" value="Default" />
      </map>
    </option>
    <option name="disabledCodeVisionProviderIds">
      <set>
        <option value="inheritors" />
        <option value="problems" />
        <option value="vcs.code.vision" />
      </set>
    </option>
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="JAVA" value="true" />
      </map>
    </option>
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value=".references" />
  </component>
  <component name="SpringEditorOptions">
    <option name="showMultipleContextsPanel" value="false" />
    <option name="showProfilesPanel" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern useCustomColors="true" pattern="\btodo\s*(must).*">
      <option name="FOREGROUND" value="ff32e7" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff3be6" />
      <option name="EFFECT_TYPE" value="-1" />
    </pattern>
    <pattern pattern="\btodo\b.*" />
  </component>
</application>