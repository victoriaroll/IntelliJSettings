<application>
  <component name="CodeInsightSettings">
    <option name="USE_INSTANCEOF_ON_EQUALS_PARAMETER" value="true" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="10" />
    <option name="LINE_SPACING" value="1.0" />
  </component>
  <component name="EditorSettings">
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
  </component>
  <component name="SpringEditorOptions">
    <option name="showMultipleContextsPanel" value="false" />
    <option name="showProfilesPanel" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern useCustomColors="true" pattern="\btodo\s*(must).*">
      <option name="FOREGROUND" value="ff32e7" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff3be6" />
      <option name="EFFECT_TYPE" value="-1" />
    </pattern>
    <pattern pattern="\btodo\b.*" />
  </component>
</application>